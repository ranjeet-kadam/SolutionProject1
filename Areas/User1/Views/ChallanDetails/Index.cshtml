@model IEnumerable<EChallan1.Web.Models.ChallanDetail>

@{
    ViewData["Title"] = "Index";
}

<h1>Challan Details</h1>

<p>
   @* <a asp-action="Create">Create New</a>*@
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UpiId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PaymentStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Challan)
